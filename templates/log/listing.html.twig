{% extends 'manage/base.html.twig' %}

{% block body %}
	<h2 class="d-flex justify-content-between align-items-center">
		<span>Log Files</span>
	</h2>
	<hr>
    {% for f in files %}
        <a href="{{url_for(f.route)}}" class="btn btn-outline-secondary btn-lg"><i class="{{f.icon}}"></i> {{f.name}}</a>
    {% endfor %}
{% endblock %}
